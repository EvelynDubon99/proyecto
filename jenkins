pipeline{
        agent any
            tools { 
                    maven 'MAVEN_HOME'
                }
            stages {
                stage('conexion') {
                    steps {

                         git 'https://github.com/EvelynDubon99/proyecto'
                    }
                }
                stage('Build') {
                    steps {
                            sh 'mvn package'
                    }

        }
    }
}
