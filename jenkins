node{
  stage ('Build') {

    git url: 'https://github.com/EvelynDubon99/proyecto'

    withMaven(
       
        maven: 'MAVEN_HOME', 
        mavenLocalRepo: '.repository', 
        mavenSettingsConfig: 'my-maven-settings' 
    ) {

      // Run the maven build
      sh "mvn clean package"

    } 
  }
}
